{
  "name": "cli-tools-manager",
  "version": "1.0.0",
  "description": "CLI tools documentation generator and manager",
  "type": "module",
  "main": "dist/cli.js",
  "bin": {
    "cli-tools": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "postbuild": "node -e \"const fs=require('fs');const p='dist/cli.js';const s=fs.readFileSync(p,'utf8');if(!s.startsWith('#!')){fs.writeFileSync(p,'#!/usr/bin/env node\\n'+s)};\" && chmod +x dist/cli.js",
    "start": "tsx src/cli.ts",
    "generate": "tsx src/cli.ts",
    "dev": "tsx watch src/cli.ts",
    "type-check": "tsc --noEmit",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "test": "echo 'No tests' && exit 0",
    "check": "npm run type-check && npm run lint && tsx src/cli.ts --stats-only --quiet",
    "validate": "tsx src/cli.ts --validate --stats-only --quiet"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^11.1.0",
    "execa": "^8.0.1",
    "remark": "^15.0.1",
    "remark-gfm": "^4.0.0",
    "remark-stringify": "^11.0.0",
    "which": "^3.0.0"
  },
  "devDependencies": {
    "@types/mdast": "^4.0.4",
    "@types/node": "^20.10.0",
    "@types/which": "^3.0.4",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "eslint": "^8.55.0",
    "prettier": "^3.1.0",
    "ts-node": "^10.9.0",
    "tsx": "^4.6.0",
    "typescript": "^5.3.0"
  },
  "keywords": [
    "cli",
    "tools",
    "documentation",
    "markdown",
    "generator"
  ],
  "author": "CLI Tools Context",
  "license": "MIT"
}
