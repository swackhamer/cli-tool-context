{
  "name": "cli-tools-manager",
  "version": "1.0.0",
  "description": "CLI tools documentation generator and manager",
  "type": "module",
  "main": "dist/cli.js",
  "bin": {
    "cli-tools": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "tsx src/cli.ts",
    "generate": "tsx src/cli.ts",
    "dev": "tsx watch src/cli.ts",
    "type-check": "tsc --noEmit",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^11.1.0",
    "execa": "^8.0.1",
    "remark": "^15.0.1",
    "remark-gfm": "^4.0.0",
    "which": "^3.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/which": "^3.0.4",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "eslint": "^8.55.0",
    "prettier": "^3.1.0",
    "ts-node": "^10.9.0",
    "tsx": "^4.6.0",
    "typescript": "^5.3.0"
  },
  "keywords": [
    "cli",
    "tools",
    "documentation",
    "markdown",
    "generator"
  ],
  "author": "CLI Tools Context",
  "license": "MIT"
}
