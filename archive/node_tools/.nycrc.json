{
  "extends": "@istanbuljs/nyc-config-typescript",
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
    "**/*.test.ts",
    "**/*.spec.ts",
    "dist/**/*",
    "node_modules/**/*",
    "coverage/**/*"
  ],
  "all": true,
  "extension": [
    ".ts"
  ],
  "reporter": [
    "text",
    "lcov",
    "json",
    "html"
  ],
  "sourceMap": true,
  "instrument": true,
  "cache": true,
  "check-coverage": true,
  "branches": 80,
  "functions": 80,
  "lines": 80,
  "statements": 80
}